<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.khk11.gallery.dao.GalleryDao">
    <insert id="insertGallery" parameterType="GalleryDto">
        insert into gallery values(
            seq_gallery.nextval, #{title}, #{description}, #{link}, #{bg}, #{original}, #{renamed}
        )
    </insert>
    <select id="getAllList" resultType="GalleryDto" >
        select * from gallery order by id desc
    </select>

</mapper>